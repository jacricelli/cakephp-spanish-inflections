{
    "name": "jacricelli/cakephp-spanish-inflections",
    "description": "Spanish Inflections plugin for CakePHP",
    "type": "cakephp-plugin",
    "version": "1.0.6",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SpanishInflections\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpanishInflections\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
